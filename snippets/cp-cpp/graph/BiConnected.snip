  // !! require Lowlink !!
  /// --- BiConnected Component Decomposition Libary {{{ ///
  
  // undirected graph
  struct BiConnected {
    Lowlink lowlink;
    vector<int> comp;
  
    TwoEdge(Lowlink lowlink): lowlink(lowlink){
      decomposite();
    }
  
    vector< vector<int> > tree;
    vector<int> used;
  
    private:
    void decomposite() {
      int size = lowlink.size;
  
      tree.resize(size);
  
      used.assign(size, 0);
      comp.assign(size, -1);
  
      int gid = 0;
      REP(i, size) if(!used[i]) dfs(i, gid++);
  
      tree.resize(gid);
    }
  
    void dfs(int i, int gid) {
      used[i] = 1;
      comp[i] = gid;
      for(auto edge : lowlink.g[i]) {
        int j = edge.to;
        int idx = edge.idx;
        if(lowlink.isBridge[idx]) {
          if(used[j]) {
            tree[gid].PB(comp[j]);
            tree[comp[j]].PB(gid);
          }
        }else if(!used[j]){
          dfs(j, gid);
        }
      }
    }
  };
  
  /// }}}--- ///

