snippet     uniq
options     head
  #define uniq(v) sort(begin(v), end(v)), (v).erase(unique(begin(v), end(v)), end(v))

snippet     smax
alias       smin
options     head
  template <class T, class U> inline void smax(T &a, U b) { a = a > b ? a : b; }
  template <class T, class U> inline void smin(T &a, U b) { a = a < b ? a : b; }

